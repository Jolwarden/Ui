            android:theme="@style/Theme.AppCompat.Light.NoActionBar"/>
        <activity
            android:name=".EssaimView"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="sensorLandscape"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"/>
        <activity
            android:name=".APropos"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"/>
        <activity
            android:name=".Notice"
            android:parentActivityName=".MainActivity"
